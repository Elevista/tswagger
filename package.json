{
  "name": "tswagger",
  "version": "1.1.0-1",
  "description": "TS-Swagger plugin generator CLI",
  "node": ">=18.0.0",
  "bin": {
    "tswagger": "dist/cli.js"
  },
  "scripts": {
    "alpha": "npm version prerelease --preid=alpha",
    "build": "tsc",
    "lint": "eslint --format friendly --ext .ts,.js .",
    "lint:fix": "eslint --format friendly --ext .ts,.js . --fix",
    "prepare": "simple-git-hooks"
  },
  "keywords": [
    "typescript",
    "swagger"
  ],
  "files": [
    "dist"
  ],
  "author": "Elevista",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "cross-fetch": "^3.0.6",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "yargs": "^16.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "types",
    "plugins"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./tsDoc": "./dist/tsDoc.js",
    "./template": "./dist/template.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.6",
    "@types/node": "^18.19.39",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "axios": "^1.3.0",
    "eslint": "^8.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-formatter-friendly": "^7.0.0",
    "simple-git-hooks": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elevista/tswagger.git"
  },
  "bugs": {
    "url": "https://github.com/Elevista/tswagger/issues"
  },
  "homepage": "https://github.com/Elevista/tswagger#readme"
}
